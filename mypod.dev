server {
    server_name mypod.dev www.mypod.dev;
    access_log  /var/log/nginx/mypod.dev.access.log;
    error_log  /var/log/nginx/mypod.dev.error.log;
    location  / {
    root /var/www/mypod.dev/export;
     index  index.html index.htm;
}



    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/mypod.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/mypod.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.mypod.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = mypod.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name mypod.dev www.mypod.dev;
    listen 80;
    return 404; # managed by Certbot




}