server {
    server_name mithril.happyhobbit.dev;
  access_log  /var/log/nginx/mithril.happyhobbit.dev.access.log;
  error_log  /var/log/nginx/mithril.happyhobbit.dev.error.log;
  location  / {
 root /var/www/mithril.happyhobbit.dev/mithril/dist;
           index index.html index.htm;
}


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/mithril.happyhobbit.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/mithril.happyhobbit.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
    
server {
    if ($host = mithril.happyhobbit.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name mithril.happyhobbit.dev;
    listen 80;
    return 404; # managed by Certbot


}
